<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gavin.aplicacaosiga" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".aplicacaoSiga"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".principal"
            android:label="@string/title_activity_principal" >
        </activity>
        <activity
            android:name=".apiario"
            android:label="@string/title_activity_apiario" >
        </activity>
        <activity
            android:name=".editarApiario"
            android:label="@string/title_activity_editar_apiario" >
            <intent-filter>
                <action android:name="EdicaoApiario" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".listarApiario"
            android:label="@string/title_activity_listar_apiario" >
        </activity>
        <activity
            android:name=".Dashboard_List"
            android:label="@string/title_activity_dashboard__list" >
        </activity>
        <activity
            android:name=".Cadastro_cliente"
            android:label="@string/title_activity_cadastro_cliente" >
        </activity>
        <activity
            android:name=".listaCliente"
            android:label="@string/title_activity_lista_cadastro" >
        </activity>
        <activity
            android:name=".editarCliente"
            android:label="@string/title_activity_editar_cliente" >
        </activity>
    </application>

</manifest>
